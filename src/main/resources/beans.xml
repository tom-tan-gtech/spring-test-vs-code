<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    <bean id="student_bean" class="com.tomtan.spring_test_vs_code.Student" >
        <constructor-arg ref="address_bean"></constructor-arg>
    </bean>

    <bean id="address_bean" class="com.tomtan.spring_test_vs_code.Address">
        <property name="address" value="Wall Street 12"></property>
    </bean>

</beans>